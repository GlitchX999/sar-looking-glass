{
  "name": "sar-earth-explorer-advanced",
  "version": "2.0.0",
  "description": "Advanced SAR data analysis platform with AI/ML capabilities for NASA Space Apps Challenge",
  "private": true,
  "type": "module",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "d3": "^7.8.5",
    "three": "^0.158.0",
    "@react-three/fiber": "^8.15.11",
    "@react-three/drei": "^9.92.7",
    "axios": "^1.5.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "@tensorflow/tfjs": "^4.11.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "lucide-react": "^0.288.0",
    "framer-motion": "^10.16.4",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^4.5.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/dist/**/*.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
